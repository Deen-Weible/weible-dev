---
const { src, alt, cssclass, style, id } = Astro.props;
---

<object data={src ? `${src}.avif` : ''} type="image/avif" class={cssclass ?? ''} style={style ?? ''} id={id ?? ''}>
<img src={src ? `${src}.jpg` : ''} alt={alt ?? ''} loading="lazy">
</object>

